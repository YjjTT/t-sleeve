+<view wx:if="{{show}}" class="{{type=='flash'?'l-class':''}} {{type + '-spinner'}} {{type=='change'||custom?'':'spinner-' + type + '-' + size}}">
+  <block wx:if="{{custom}}">
+    <slot />  
+  </block>
+  <block wx:else>
