+Component({
+  externalClasses: ['l-class'],
+
+  properties: {
+    type: {
+      type: String,
+      value: 'still'
+    },
+    // 轮播数组
+    swipArr: Array,
+    // 前置图标
+    frontIconName: {
+      type: String,
+      value: ''
+    },
+    frontIconSize: {
+      type: Number,
+      value: 28
+    },
+    frontIconColor: {
+      type: String,
+      value: '#3683D6'
+    },
+    endIconName: {
+      type: String,
+      value: ''
+    },
+    endIconSize: {
+      type: Number,
+      value: 28
