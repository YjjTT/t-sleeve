+      this.setData({
+        _isHaveUserNickName: openData.indexOf('userNickName') !== -1
+      });
+    },
+    tapAvatar: function (e) {
+      this.triggerEvent('lintap', e, {
+        bubbles: true,
+        composed: true
+      });
+    },
+  }
+});
