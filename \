+<!--  input/input.wxml -->
+<label class='form-item {{disabled? "disabled": ""}}   l-class '>
+  <view class='default-border {{border? "border": ""}}'>
+    <view class='mask' wx:if="{{disabled}}"></view>
+    <!-- 小程序表单组件 -->
